root ::= event+

event ::= actorreply newline end

actorreply ::= "EventType: CharacterReply" newline "Codename:" codename newline "LookingAtCodename: " codename newline emotionline

newline ::= "\n"

codename ::= alphanumeric*

emotionline ::= "Emotion: " emotion newline "Line: " line

emotion ::= "neutral" | "surprised" | "thinking" | "confident"

expression ::= alphanumeric*

line ::= "'" alphanumeric* "'"

end ::= "$[END]"

alphanumeric ::= [a-zA-Z0-9 !#%&'()*+,-./:;<=>?@\[\]^_`{|}~]